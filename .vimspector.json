{
  "configurations": {
    "Launch": {
      "adapter": "vscode-cpptools",
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/out/catchme",
        "args": [ ],
        "cwd": "${workspaceRoot}",
        "environment": [ ],
        "externalConsole": true,
        "MIMode": "gdb"
      }
    },
    "Attach": {
      "adapter": "vscode-cpptools",
      "configuration": {
        "request": "attach",
        "program": "${workspaceRoot}/out/catchme",
        "MIMode": "gdb"
      }
    }
  }
}
